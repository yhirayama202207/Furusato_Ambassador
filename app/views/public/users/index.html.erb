<h5>public/users/index</h5>

</br>
<div class= "col-md-8 offset-md-1">
  <h5>ユーザー一覧</h5>
  <div style="text-align:center;">
  <table class="table table-hover table-inverse">
    <thead>
      <tr>
        <th>ユーザーネーム</th>
        <th>出身地</th>
        <th colspan="4"></th>
      </tr>
    </thread>
    <tbody>
        <% @users.each do |user| %>
        <tr>
        <td>
          <%= link_to admin_user_path(user.id) do %>
          <%= user.user_name %>
          <% end %>
        </td>
        <td>
          <%= user.hometown %>
        </td>

        </tr>
        <% end %>
    </tbody>
  </table>
  </div>

  <%= paginate @users %>