<div class= "row">
<%= render 'public/articles/list', japan_prefectures: @japan_prefectures %>

  <div class= "col-10">
  </br>
  <h5><%= @japan_prefecture.name %>の記事（全<%= @japan_prefecture.articles.count %>件）</h5>

    <div class= "row">
    <% @articles.each do |article| %>
      <div class= "col-3">

      <%= render 'public/articles/article_part', article: article %>

      </div>
    <% end %>
    </div>
  </div>
</div>